body 
	margin:0;
	padding:0;

div#top
	width:100%
	height:33%
	background-color:#333

div#bottom
	width:100%
	height:33%
	bottom:0
	background-color:#555
	// padding: 10px

div#middle
	background-color:#111
	width:100%
	color:white
	height:34%
	font-size: 0px
	letter-spacing: 0px
	word-spacing: 0px

	.section
		width: 20%
		height: 100%
		float:left
		box-sizing: border-box
		border: 1px dotted #ccc
		position: relative

		&.ui-state-hover
			background: #000

		&.ui-state-active
			background: #fff

		.card
			display: inline-block
			position: absolute

			&:nth-child(2)
				left: 30px
			&:nth-child(3)
				left: 60px
			&:nth-child(4)
				left: 90px
			&:nth-child(5)
				left: 120px
			&:nth-child(6)
				left: 150px
			&:nth-child(7)
				left: 180px
			&:nth-child(8)
				left: 210px
			&:nth-child(9)
				left: 240px
			&:nth-child(10)
				left: 270px

